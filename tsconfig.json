{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["frontend/src/*"],
      "@types": ["frontend/src/types.ts"]
      "@components/*": ["frontend/components/*"],
      "@details/*": ["frontend/components/property_details/*"],
      "@map/*": ["frontend/src/app/*"],
      "@lib/*": ["frontend/lib/*"]
    },
    "plugins": [{ "name": "next" }]
  },
  "include": [
    "frontend",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
